    "@types/shelljs": "^0.8.15",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "release-it": "^19.0.0",
    "rollup": "^4.36.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "shelljs": "^0.10.0",
    "ts-jest": "^29.2.6",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2"
